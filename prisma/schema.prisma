generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid())
  name      String
  email     String
  cpf       String
  password  String
  createdAt String @map("created_at")

  @@map("user")
}
