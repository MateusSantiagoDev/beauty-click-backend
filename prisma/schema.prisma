generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id
  name      String
  email     String
  cpf       String
  password  String
  role      String
  cep       String
  district  String
  road      String
  number    Int
  createdAt DateTime? @map("created_at")
  updatedAt DateTime? @map("updated_at")

  @@map("user")
}
